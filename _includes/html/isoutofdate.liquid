{% assign isoutofdate = false %}
{% capture datenum %}{{ post.date | date: '%y%m%d' }}{% endcapture %}
{% unless page.begin == null %}
  {% capture begindiff %}{{ datenum | minus:page.begin }}{% endcapture %}
{% endunless %}
{% unless page.end == null %}
  {% capture enddiff %}{{ page.end | minus:datenum }}{% endcapture %}
{% endunless %}
{% if begindiff contains '-' or enddiff contains '-' %}
  {% assign isoutofdate = true %}
{% endif -%}
